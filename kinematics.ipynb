    "# Make a plotting canvas\n",
    "canvas = tp.Canvas(600,400)\n",
    "# Add some axes\n",
    "axes = canvas.cartesian()\n",
    "# Plot the two graphs\n",
    "velocity_graph = axes.plot(ts,vs)\n",
    "position_graph = axes.plot(ts,xs)\n",
    "# Which one's which?\n",
    "canvas.legend([(\"Position\",position_graph),(\"Velocity\",velocity_graph)]);"
   ]
